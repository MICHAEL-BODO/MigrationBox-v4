{
  "name": "migrationbox-v4",
  "version": "4.1.0",
  "description": "MigrationBox - Enterprise Multi-Cloud Migration Automation Platform",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "test": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:localstack": "jest --testPathPattern=tests/localstack",
    "dev": "serverless offline",
    "deploy:local": "serverless deploy --stage local",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:staging": "serverless deploy --stage staging",
    "deploy:production": "serverless deploy --stage production",
    "localstack:up": "docker compose -f infrastructure/docker/docker-compose.localstack.yml up -d",
    "localstack:down": "docker compose -f infrastructure/docker/docker-compose.localstack.yml down",
    "localstack:health": "curl -s http://localhost:4566/_localstack/health | jq",
    "fullstack:up": "docker compose -f infrastructure/docker/docker-compose.yml up -d",
    "fullstack:down": "docker compose -f infrastructure/docker/docker-compose.yml down"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.500.0",
    "@aws-sdk/client-s3": "^3.500.0",
    "@aws-sdk/client-sqs": "^3.500.0",
    "@aws-sdk/client-sns": "^3.500.0",
    "@aws-sdk/client-sfn": "^3.500.0",
    "@aws-sdk/client-lambda": "^3.500.0",
    "@aws-sdk/client-iam": "^3.500.0",
    "@aws-sdk/client-sts": "^3.500.0",
    "@aws-sdk/client-secretsmanager": "^3.500.0",
    "@aws-sdk/client-eventbridge": "^3.500.0",
    "@aws-sdk/lib-dynamodb": "^3.500.0",
    "@azure/storage-blob": "^12.17.0",
    "@azure/cosmos": "^4.0.0",
    "@azure/service-bus": "^7.9.0",
    "@azure/identity": "^4.0.0",
    "@azure/arm-compute": "^21.0.0",
    "@azure/arm-storage": "^18.0.0",
    "@azure/arm-sql": "^10.0.0",
    "@azure/arm-cosmosdb": "^16.0.0",
    "@azure/arm-network": "^33.0.0",
    "@azure/arm-appservice": "^15.0.0",
    "@azure/arm-containerservice": "^20.0.0",
    "@google-cloud/storage": "^7.7.0",
    "@google-cloud/firestore": "^7.3.0",
    "@google-cloud/pubsub": "^4.3.0",
    "@google-cloud/compute": "^4.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.5.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0",
    "serverless": "^4.0.0",
    "serverless-offline": "^13.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "UNLICENSED",
  "private": true
}
