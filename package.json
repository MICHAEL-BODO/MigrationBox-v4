{
  "name": "migrationbox-v5",
  "version": "5.0.0",
  "description": "MigrationBox V5.0 - AI-First Multi-Cloud Migration Platform with I2I Pipeline, Agentic AI, CRDT Knowledge, Extended Thinking & MCP Mesh",
  "private": true,
  "workspaces": [
    "packages/*",
    "services/*",
    "frontend/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test": "jest",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:integration": "jest --testPathPattern=integration",
    "test:localstack": "jest --testPathPattern=localstack",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules",
    "localstack:up": "docker compose -f infrastructure/docker/docker-compose.localstack.yml up -d",
    "localstack:down": "docker compose -f infrastructure/docker/docker-compose.localstack.yml down",
    "localstack:health": "curl -s http://localhost:4566/_localstack/health | jq",
    "fullstack:up": "docker compose up -d",
    "fullstack:down": "docker compose down"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/client-sqs": "^3.490.0",
    "@aws-sdk/client-sns": "^3.490.0",
    "@aws-sdk/client-sfn": "^3.490.0",
    "@aws-sdk/client-lambda": "^3.490.0",
    "@aws-sdk/client-iam": "^3.490.0",
    "@aws-sdk/client-sts": "^3.490.0",
    "@aws-sdk/client-secrets-manager": "^3.490.0",
    "@aws-sdk/client-eventbridge": "^3.490.0",
    "@aws-sdk/lib-dynamodb": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@aws-sdk/util-dynamodb": "^3.490.0",
    "@azure/storage-blob": "^12.17.0",
    "@azure/cosmos": "^4.0.0",
    "@azure/service-bus": "^7.9.0",
    "@azure/identity": "^4.0.0",
    "@azure/arm-compute": "^21.0.0",
    "@azure/arm-storage": "^18.0.0",
    "@azure/arm-sql": "^10.0.0",
    "@azure/arm-cosmosdb": "^16.0.0",
    "@azure/arm-network": "^33.0.0",
    "@azure/arm-appservice": "^15.0.0",
    "@azure/arm-containerservice": "^20.0.0",
    "@google-cloud/storage": "^7.7.0",
    "@google-cloud/firestore": "^7.3.0",
    "@google-cloud/pubsub": "^4.3.0",
    "@google-cloud/compute": "^4.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0",
    "serverless": "^3.38.0",
    "serverless-offline": "^13.9.0",
    "turbo": "^2.0.0",
    "prettier": "^3.2.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "npm@10.0.0",
  "license": "UNLICENSED"
}
